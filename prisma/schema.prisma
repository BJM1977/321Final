generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL") // kommt aus deiner .env-Datei
shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

model Message {
  id        Int      @id @default(autoincrement())
  text      String
  username  String
  createdAt DateTime @default(now())
}
